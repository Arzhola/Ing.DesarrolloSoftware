//Calculadora al reves
#include <stdio.h>
int main() {
    int num,num1,num2,num3,num4;
    printf ("Ingresa un numero de 4 digitos:");
    scanf ("%d",&num);
    num4=num%10;
    num3=(num%100)/10;
    num2=(num%1000)/100;
    num1=num/1000;
    printf ("Tu numero al reves es:\n%d%d%d%d",num4,num3,num2,num1);
    return 0;
}
